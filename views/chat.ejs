<div id='chat_tooltip' class='context_menu'>
    <ul>
        <li><%= gettext('Copy') %></li>
        <li><%= gettext('Translate') %></li>
    </ul>
</div>
<div id='chat_window'>
    <div id='chat_messages'>
        <ul></ul>
    </div>
    <div id='send_area'>
        <form id='send_form'>
            <button class='message_button uk-icon-circle-o-notch uk-icon-spin' id='wait_preloader'></button>
            <button class='message_button uk-icon-plus new clickable' id='new_button'></button>
            <button class='message_button uk-icon-send send clickable' id='send_button'></button>
            <span><input  id='send_box' type='text' autocomplete='off'></input></span>
            <button class='message_button uk-icon-times disconnect clickable' id='disconnect_button'></button>
        </form>
    </div>
</div>
